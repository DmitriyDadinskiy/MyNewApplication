<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button_4"
            motion:layout_constraintBottom_toTopOf="@+id/button_8"
            android:layout_width="wrap_content"
            android:id="@+id/button_5" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button_1"
            motion:layout_constraintBottom_toTopOf="@+id/button_5"
            android:layout_width="wrap_content"
            android:id="@+id/button_2" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button_clear"
            motion:layout_constraintBottom_toTopOf="@+id/button_2"
            android:layout_width="wrap_content"
            android:id="@+id/button_multiply" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button_multiply"
            motion:layout_constraintBottom_toTopOf="@+id/button_3"
            android:layout_width="wrap_content"
            android:id="@+id/button_divide" />
        <Constraint
            android:layout_height="200dp"
            android:layout_width="260dp"
            android:id="@+id/textView"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.496"
            android:layout_marginEnd="28dp"
            android:layout_marginRight="28dp" />
        <Constraint
            android:layout_height="38dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.933"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="259dp"
            android:id="@+id/button9"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.428"
            motion:layout_constraintTop_toBottomOf="@+id/textView" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/button_9"
            motion:layout_constraintStart_toEndOf="@+id/button_8"
            motion:layout_constraintBottom_toTopOf="@+id/button_comma" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/button_8"
            motion:layout_constraintStart_toEndOf="@+id/button_7"
            motion:layout_constraintBottom_toTopOf="@+id/button_0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/button_percent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/button_1"
            android:layout_width="wrap_content"
            android:id="@+id/button_clear"
            android:layout_marginBottom="0dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/button_4"
            android:layout_width="wrap_content"
            android:id="@+id/button_1"
            android:layout_marginBottom="0dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button_6"
            motion:layout_constraintBottom_toTopOf="@+id/button_equally"
            android:layout_width="wrap_content"
            android:id="@+id/button_plus"
            android:layout_marginBottom="1dp"
            android:layout_marginLeft="0dp"
            android:layout_marginStart="0dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button_3"
            motion:layout_constraintBottom_toTopOf="@+id/button_plus"
            android:layout_width="wrap_content"
            android:id="@+id/button_minus"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginStart="0dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button_divide"
            motion:layout_constraintBottom_toTopOf="@+id/button_minus"
            android:layout_marginBottom="0dp"
            android:layout_width="wrap_content"
            android:id="@+id/button_delete"
            android:layout_marginLeft="0dp"
            android:layout_marginStart="0dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>